<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Inherited Employee Form view-->
    <record id="inherit_employee_form_sales_force_form_olila" model="ir.ui.view">
        <field name="name">hr.employee.sales.force.olila</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="dsl_employee_access.view_employee_form_sales_force"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//field[@name='active_status']" position="after">
                    <field name="division_id"/>
                    <field name="district_id"/>
                    <field name="upazila_id"/>
                    <field name="zone_id"/>
                    <field name="territory_id"/>
                    <field name="area_id"/>
                </xpath>
            </data>

        </field>
    </record>

    <record id="hr_employee_view_form_inherit_olila" model="ir.ui.view">
        <field name="name">hr.employee.form.seniority.olila</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr_employee_seniority.hr_employee_view_form_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//group/field[@name='length_of_service']" position="before">
                <field name="is_quarterly_assessment"/>
                <label for="probation_duration"/>
                <div class="o_row" name="div_km_home_work">
                    <field name="probation_duration" groups="hr.group_hr_user"/>
                    <span>Months</span>
                </div>
            </xpath>
        </field>
    </record>
    <record id="hr_employee_view_form_olila" model="ir.ui.view">
        <field name="name">hr.employee.view.form</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr_employee_age.hr_employee_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='age']" position="replace"/>
        </field>
    </record>

    <record id="hr_employee_view_form_family" model="ir.ui.view">
        <field name="name">hr.employee.view.form</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr_family.hr_employee_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page/group/group/field[@name='fam_spouse']" position="after">
                <field name="spouse_complete_name"/>
                <field name="spouse_birthdate"/>
            </xpath>
            <xpath expr="//page/group/group[@name='fam_parents']" position="after">
                <group string="Dependant" attrs="{'invisible':[('marital','=','single')]}">
                    <field name="children"/>
                </group>
            </xpath>
        </field>
    </record>

    <record id="inherit_employment_history_view_form" model="ir.ui.view">
        <field name="name">Employment History info inherit</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr_employment_history.employment_history_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page/group/field/tree/field[@name='clearance_certification']" position="after">
                <field name="last_salary_drawn"/>
                <field name="reasons_for_leaving"/>
                <field name="address"/>
            </xpath>
        </field>
    </record>

    <!-- Inherited Employee Form view-->
    <record id="inherit_hr_employee_form_olila" model="ir.ui.view">
        <field name="name">hr.employee.view.form.olila</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='initial_employment_date']" position="attributes">
                <attribute name="string">Date of Joining</attribute>
            </xpath>
            <xpath expr="//field[@name='default_operating_unit_id']" position="attributes">
                <attribute name="string">Job Location</attribute>
            </xpath>
            <xpath expr="//field[@name='department_id']" position="attributes">
                <attribute name="string">Section Name</attribute>
            </xpath>
            <xpath expr="//field[@name='emergency_phone']" position="after">
                <field name="relation"/>
            </xpath>
            <xpath expr="//page[@name='personal_information']/group/group/field[@name='address_home_id']"
                   position="replace">
                <field name="address_home_id"
                       context="{'show_address': 1,
                                     'default_type': 'private',
                                     'form_view_ref': 'base.res_partner_view_form_private'}"
                       options='{"always_reload": True, "highlight_first_line": True}' invisible="1"/>
                <field name="permanent_address"/>
                <field name="present_address"/>
            </xpath>
            <xpath expr="//page[@name='personal_information']/group/group/field[@name='bank_account_id']"
                   position="replace">
                <field name="bank_account_id"/>
            </xpath>
            <xpath expr="//page[@name='hr_settings']/group/group/field[@name='job_id']" position="after">
                <field name="is_eligible_bonus"/>
                <field name="is_previously_applied"/>
            </xpath>
            <xpath expr="//field[@name='passport_id']" position="replace"/>
            <xpath expr="//field[@name='gender']" position="replace"/>
            <xpath expr="//field[@name='birthday']" position="replace"/>

            <xpath expr="//page[@name='personal_information']/group/group[3]" position="replace">
                <group string="Personal Information" name="personal_info">
                    <field name="gender"/>
                    <field name="birthday"/>
                    <field name="age"/>
                    <field name="marital"/>
                    <field name="marriage_date"
                           attrs="{'invisible': [('marital', 'not in', ['married', 'cohabitant'])]}"/>
                    <field name="religion"/>
                    <field name="blood_group"/>
                    <field name="hobbie_interest" widget="many2many_tags"/>
                    <field name="is_emp_training" invisible="1"/>
                </group>
            </xpath>
            <xpath expr="//page[@name='personal_information']/group/group[4]" position="replace"/>


            <xpath expr="//page[@name='personal_information']/group/group[5]" position="replace">
                <group string="Work Permit" name="work_permit">
                    <field name="work_permit" widget="boolean_toggle"/>
                    <field name="visa_no" attrs="{'invisible': [('work_permit', '=', False)]}"/>
                    <field name="permit_no" attrs="{'invisible': [('work_permit', '=', False)]}"/>
                    <field name="wp_place_issue" attrs="{'invisible': [('work_permit', '=', False)]}"/>
                    <field name="visa_expire" attrs="{'invisible': [('work_permit', '=', False)]}"/>
                </group>
                <group string="Passport" name="passport_no">
                    <field name="passport" widget="boolean_toggle"/>
                    <field name="passport_id" attrs="{'invisible': [('passport', '=', False)]}"/>
                    <field name="date_issue" attrs="{'invisible': [('passport', '=', False)]}"/>
                    <field name="passport_issue_place" attrs="{'invisible': [('passport', '=', False)]}"/>
                    <field name="date_expiry" attrs="{'invisible': [('passport', '=', False)]}"/>
                </group>
            </xpath>

            <xpath expr="//page[@name='hr_settings']" position="after">
                <page string="Professional Qualifications" groups="base.group_user">
                    <group>
                        <field name="professional_qualification_ids" nolabel="1">
                            <tree editable="top" string="History">
                                <field name="pq_certification_name"/>
                                <field name="pq_certificate_number"/>
                                <field name="pq_institute"/>
                                <field name="pq_country_id"/>
                                <field name="pq_expiry_date"/>
                            </tree>
                        </field>
                    </group>
                </page>
                <page string="License Detail" groups="base.group_user">
                    <group string="License" name="license">
                        <field name="other_professional_license_ids" nolabel="1">
                            <tree editable="top" string="License">
                                <field name="license_no"/>
                                <field name="license_type"/>
                                <field name="license_issue_date"/>
                                <field name="license_expiry_date"/>
                                <field name="given_by"/>
                            </tree>
                        </field>
                    </group>
                </page>
                <page string="Training" groups="base.group_user">
                    <group>
                        <field name="training_other_ids" nolabel="1">
                            <tree editable="top" string="Training">
                                <field name="training_institute_name"/>
                                <field name="to_course_name"/>
                                <field name="date_from"/>
                                <field name="date_to"/>
                                <field name="training_duration"/>
                                <field name="training_result"/>
                                <field name="training_location"/>
                                <field name="training_country_id"/>
                            </tree>
                        </field>
                    </group>
                </page>
                <page string="Incident" groups="hr.group_hr_user">
                    <group>
                        <field name="incident_line_ids" nolabel="1">
                            <tree string="List" editable="bottom">
                                <field name="person_id"/>
                                <field name="person_no"/>
                                <field name="department_id"/>
                                <field name="job_id"/>
                                <field name="incident_date"/>
                                <field name="incident_location"/>
                                <!--                                    <field name="incident_time"/>-->
                                <field name="incident_detail"/>
                                <field name="remarks"/>
                            </tree>
                        </field>
                    </group>
                </page>
                <page string="Employee Training" groups="hr.group_hr_user">
                    <group>
                        <field name="filtered_training_line_ids" nolabel="1" readonly="1">
                            <tree string="List" editable="bottom">
                                <field name="program_id"/>
                                <field name="training_id"/>
                                <field name="training_name"/>
                                <field name="start_date"/>
                                <field name="end_date"/>
                                <field name="duration"/>
                                <field name="state"/>
                            </tree>
                        </field>
                    </group>
                </page>
                <page string="Relatives in Olila Group" groups="hr.group_hr_user">
                    <group>
                        <field name="olila_relative_ids" nolabel="1">
                            <tree editable="top" string="Relative">
                                <field name="relative_name"/>
                                <field name="job_id"/>
                                <field name="department_id"/>
                                <field name="relationship"/>
                            </tree>
                        </field>
                    </group>
                </page>
                <page string="Declaration" groups="hr.group_hr_user">
